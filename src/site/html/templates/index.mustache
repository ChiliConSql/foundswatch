    <div class="grid-container" style="padding-top: 3.5rem;">
        <div class="grid-x grid-margin-x">
            {{#themeViews}}
            <div class="cell medium-4" style="margin-top: 2rem;">
                <div class="card">
                    <a href="{{root}}/{{dir}}">
                        <img src="assets/img/thumbnails/{{dir}}.png">
                    </a>
                    <!--
                    <a href="{{root}}/{{dir}}">
                        <img src="http://via.placeholder.com/1000x600/483D3F/ffffff/?text={{name}}">
                    </a>
                    <div class="responsive-embed">
                        <iframe src="{{root}}/{{dir}}/thumbnail.html" frameborder="0" width="360" height="200" scrolling="no"></iframe>
                    </div>
                    
                    -->
                    <div class="card-section text-center" style="background-color: #e6e6e6;">
                        <h4>
                            <a href="{{root}}/{{dir}}">{{name}}</a>
                            <!--
                            <a href="{{root}}/{{dir}}/thumbnail.html">üìù</a>
                            -->
                        </h4>
                        <p>{{tagline}}</p>
                        <div>
                            <a class="primary button" href="{{root}}/{{dir}}">Preview</a>
                            <a class="primary button fs-download-button" href="{{root}}/css/themes/{{dir}}/foundation.min.css">Download</a>
                        </div>
                    </div>
                </div>
            </div>
            {{/themeViews}}
        </div>
        <div class="grid-x grid-margin-x">
            <div class="cell small-12" style="margin-top: 2rem;">
                <h3>Want to level up your skills?</h3>

            </div>
        </div>
        <div class="grid-x grid-margin-x">
            <div class="cell medium-4" style="margin-top: 2rem;">
                <div class="card">
                    <a href="https://amzn.to/2MB67aI">
                        <img src="assets/img/books/html-css-book.png">
                    </a>
                    <div class="card-section text-center" style="background-color: #e6e6e6;">
                        <h4>
                            <a href="https://amzn.to/2MB67aI">A Smarter Way to Learn HTML &amp; CSS</a>
                        </h4>
                        <p>Mark Myers</p>
                        <div>
